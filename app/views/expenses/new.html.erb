
<%= form_for :account do |f| %>

    <div class="field">
      <%= f.label :item %><br>
      <%= f.text_field :item %>
    </div>

    <div class="field">
      <%= f.label :amount %><br>
      <%= f.text_field :amount, :placeholder=>"$" %>
    </div>

    <div class="field">
      <%= f.label :category %><br>
      <%= f.text_field :category %>
    </div>

  
    <div class="field">
      <%= f.label :created_at %><br>
      <%= f.text_field :created_at %>
    </div>


    <div class="field">
      <%= f.label :account_id %><br>
      <%= f.text_field :account_id %>
    </div>


    <div class="field">
      <%= f.label :user_id %><br>
      <%= f.text_field :user_id %>
    </div>

     <p>
    <%= f.submit %>
  </p>


<% end %>


